            android:text="TextView" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ftglpeminjaman"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtdmotortglpeminjaman"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/left_vertical"
            android:text="TextView" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/ftglpengembalian"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txtdmotortglpengembalian"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView" />
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>